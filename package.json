{
  "name": "moving4u",
  "version": "0.1.0",
  "description": "Landing page for Moving 4U (static site)",
  "license": "MIT",
  "scripts": {
    "lint": "npx eslint .",
    "test": "jest --coverage",
    "test:e2e": "playwright test",
    "sitemap": "node scripts/generate-sitemap.js",
    "start": "npx http-server -p 8000"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/e2e/"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "jest": "^29.0.0",
    "jest-axe": "^6.0.1",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^2.0.0"
  },
  "dependencies": {
    "request": "^2.88.2"
  }
}
